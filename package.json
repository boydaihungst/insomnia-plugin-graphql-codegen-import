{
  "name": "insomnia-plugin-graphql-codegen-import",
  "version": "0.0.1",
  "private": true,
  "insomnia": {
    "name": "graphql-codegen-import",
    "description": "",
    "images": {
      "icon": "icon.svg",
      "cover": "cover.svg"
    }
  },
  "main": "main.js",
  "scripts": {
    "build": "esbuild main.ts --bundle --outfile=main.js",
    "dev": "esbuild main.ts --bundle --outfile=main.js --watch --platform=node",
    "download-insomnia": "esbuild download-insomnia-executable.ts --bundle --outfile=scripts/download.js --platform=node --target=node12.18.3 && node ./scripts/download.js",
    "test": "npx playwright test"
  },
  "dependencies": {
    "@graphql-codegen/core": "^2.1.0",
    "@graphql-tools/utils": "^8.3.0",
    "@playwright/test": "^1.15.2",
    "esbuild": "^0.13.5",
    "graphql": "^15.6.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/unzipper": "^0.10.4",
    "@types/uuid": "^8.3.1",
    "playwright": "^1.15.2",
    "unzipper": "^0.10.11"
  }
}
